<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Team.
    -->
    <changeSet id="20201026012903-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="sport_id"
                                 baseTableName="team"
                                 constraintName="fk_team_sport_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sport"/>

        <addForeignKeyConstraint baseColumnNames="team_id"
                                 baseTableName="team_league"
                                 constraintName="fk_team_league_team_id"
                                 referencedColumnNames="id"
                                 referencedTableName="team"/>

        <addForeignKeyConstraint baseColumnNames="league_id"
                                 baseTableName="team_league"
                                 constraintName="fk_team_league_league_id"
                                 referencedColumnNames="id"
                                 referencedTableName="league"/>
        
    </changeSet>
</databaseChangeLog>
