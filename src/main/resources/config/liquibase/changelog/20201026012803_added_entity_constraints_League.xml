<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity League.
    -->
    <changeSet id="20201026012803-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="sport_id"
                                 baseTableName="league"
                                 constraintName="fk_league_sport_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sport"/>

        <addForeignKeyConstraint baseColumnNames="region_id"
                                 baseTableName="league"
                                 constraintName="fk_league_region_id"
                                 referencedColumnNames="id"
                                 referencedTableName="region"/>

    </changeSet>
</databaseChangeLog>
